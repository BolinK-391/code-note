#include "stdio.h"


char* my_strstr(const char* str1,const char* str2){
    const char* s1 = NULL;
    const char* s2 = NULL;
    char* cp = (char*)str1;

    while(*cp){
        s1 = cp;
        s2 = str2;
        while(*s1 && *s2 && (*s1 == *s2)){
            s1++;
            s2++;
        }
        if(*s2 == '\0'){
            return cp;
        }
        cp++;
    }
    return NULL;
}

int main(){
    char arr1[] = "abcdefabcdef";
    char arr2[] = "bcdq";
    //ÔÚarr1ÖÐ²éÕÒÊÇ·ñ°üº¬arr2Êý×é
    char* ret = my_strstr(arr1,arr2);
    if(ret == NULL)
        printf("no\n");
    else
        printf("yes:%s",ret);
    return 0;
}
