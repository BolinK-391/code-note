#include <stdio.h>
//计算n的阶乘
int main() {
    int i,j=1,z=1;
    scanf("%d",&i);
    for(int num = 1;num<=i;num++){
    //     z = z*num;
    //     j = z ;
    //简便写法 将z乘上num后再把乘完之后的数字赋给z！
        z *= num ;

        printf("%d",num);
        if(num == i){
            continue;
        }
        printf("*");
    }
    printf("=%d",z);
    }

//关于赋值操作符有  = += -= *= /= >>= <<= %=
//(后7为复合赋值操作符)

//连续赋值从右向左
//a = x = y+1 (先把y+1的值赋给x然后x再赋值给a) 

//a = a + 100 等价于 a += 100（其他操作符可以等同"把 + 改掉"）

//注意：= >> 赋值
//     == >> 判断相等



